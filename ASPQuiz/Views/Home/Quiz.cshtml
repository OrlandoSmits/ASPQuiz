@model IEnumerable<ASPQuiz.Domain.Entities.Question>

@{
    ViewBag.Title = "Quiz";
}

<h2>Quiz</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionText)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Answer1)</td><td>
			@Html.DisplayFor(modelItem => item.Answer2)</td><td>
			@Html.DisplayFor(modelItem => item.CorrectAnswer)
        </td>
    </tr>
}

</table>
